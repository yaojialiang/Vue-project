#M-items{
    height: 100%;display:flex;flex-direction:column;
    .header{
        height: 1.2rem;border-bottom:1px solid #333;display:flex;font-size:0.586667rem;
        a{padding: 0.133333rem;font-size:0.453333rem;line-height: 0.933333rem;color:#0cf;}
        i{width: 1.333333rem;text-align: center;line-height: 1.2rem;color:#0cf;font-weight: normal;}
        .search{
            flex:1;display:flex;justify-content:center;position: relative;align-items:center;
            input{width: 5.366667rem;height: 0.846667rem;padding: 0 0.866667rem 0 0.266667rem;border:0 none;background: #eee;font-size:0.466667rem;border-radius:0.133333rem;}
            i{position: absolute;right: 0.65rem;top: 0;font-size:0.533333rem;}
        }
    }
    .nav{
        height: 1.333333rem;
    }
    .main{
        flex:1;
    }
}