DROP TABLE IF EXISTS `project`;
CREATE TABLE `project` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `publishMan` int(11) NOT NULL COMMENT '发布人',
  `projectName` varchar(255) NOT NULL COMMENT '团队名',
  `projectType` varchar(255) NOT NULL COMMENT '项目类型',
  `description` longtext NOT NULL COMMENT '项目简介',
  `teamNumber` int(11) NOT NULL COMMENT '招募总人数',
  `likes` int(11) NOT NULL DEFAULT '0' COMMENT '点赞数',
  `publishTime` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '发布时间',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=20 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Records of project
-- ----------------------------
INSERT INTO `project` VALUES ('1', '3', '兼职猫——找兼职APP', '工具软件', '面对密集繁多的兼职，用户找自己想要的兼职不容易，所以在技术上革新按信息算法来帮用户找出他所要的兼职。让用户来揪出黑心中介虚假岗位，让同学来分享自己的工作心得，打造更合理的兼职信息平台。', '8', '24', '2018-03-01 10:47:12');
INSERT INTO `project` VALUES ('2', '4', '超级课程表——大学生必备APP', '社交', '软件内置许多实用功能，查看课表、记录课堂笔记、成绩查询等问题统统解决；还有配置了不少服务型功能，社团活动、二手交易、失物招领等应有尽有。还能根据以往每堂课老师的点名频率进行点名预测。课表交友新方式，可以向同班同学发送私信，帮助同学认识到同一节课任意教室范围内的同学，方便同学间即时、便捷的联系交流。扩展交际圈，一手掌握校园新鲜事。', '10', '5', '2018-03-20 10:47:12');
INSERT INTO `project` VALUES ('3', '1', '创业星——大学生创业辅助平台', '互联网', '一款面向高校大学生的创业辅助平台，帮组有创业梦想的大学生完成创业梦。APP内主要功能为：让有创业idea的同学发布自己的项目，让志同道合的同学加入到项目中来，一起组建创业团队，一起去为创业梦想拼搏。当然，如果你想创业缺没有什么好的idea,你可浏览在本平台发布的项目，选择自己感兴趣的项目，向本项目发布者（队长）申请加入项目，队长审核成功后，你将成为团队的一员。此外，该APP还具有“校园创业之星”、“创业大咖”、“创业书籍推荐”、“创业新闻”、“创业大赛”等多个副功能。', '7', '17', '2018-03-25 11:03:42');
INSERT INTO `project` VALUES ('4', '5', '伏牛堂——传统米粉店', '餐饮', '卖米粉，有材有料有技术，打造中国最好吃的米粉店；用互联网思维经营：1、借助互联网吸引人流。2、借助互联网精确找到消费者。3、借助互联网保持核心竞争力。', '3', '3', '2018-03-26 11:11:21');
INSERT INTO `project` VALUES ('5', '6', 'High女性社区\r\n', '社区服务', '女性移动问答社区App，解决两性关系。女性追求性感，喜欢穿TOPSHOP、AmericanAppreal等运动风的“短小紧露”，买古董Chanel首饰...High女性社区可解决你的烦恼。', '6', '0', '2018-03-29 11:11:21');
INSERT INTO `project` VALUES ('6', '7', '聚美优品', '电子商务', '做一个化妆品限时特卖商城，解决大众选化妆品和购买化妆品的难题。网站将设置母婴专区、美妆商城、国际轻奢、运动服装、鞋包配饰等多个专区。商城商品应有尽有！', '15', '1', '2018-04-01 11:45:42');
INSERT INTO `project` VALUES ('7', '8', '在绍兴——公益红娘', '社交', '开设一个微信公众号，免费帮单身男女寻找“另一半”。先按每个人提出的“相亲要求”，团队进行筛选把关，感觉双方条件匹配，就让双方交换微信。此外，还附加有“泡妞秘籍”、“梳妆打扮”、“教你如何绅士”等附属功能。', '5', '0', '2018-04-03 11:52:54');
INSERT INTO `project` VALUES ('8', '9', '牧马人园艺', '新农业', '园艺作物种植及培育（不含林木种子）；园林景观工程、装饰装修工程的设计及施工；园林绿化景观、建筑装饰材料的研发及咨询；企业形象策划、品牌设计、图文设计制作、展览展示；施工:建筑智能化工程、市政工程、暖通工程、消防工程；园艺作物养护管理；销售、租赁：建筑机械设备、园艺作物（不含林木种子）。', '9', '0', '2018-04-04 11:55:26');
INSERT INTO `project` VALUES ('9', '10', '财经快递——大学校园里的快递', '物联网', '和快递公司合作，在校园里开办自己的一个快递点，专门为学生接收快递和送快递。', '4', '0', '2018-04-06 12:09:07');
INSERT INTO `project` VALUES ('10', '11', '宅零宅食——送上门的零食', '生活消费', '进购一批零食，在微信公众号以及朋友圈等社交平台上经营宣传，供同学们购买，学生下单成功，亲自送上宿舍。另外，还可在每个宿舍放置一个零食箱，内放一些常吃的零食，同学要吃时只需微信扫码付款相应的金额。', '11', '3', '2018-04-07 12:13:43');
INSERT INTO `project` VALUES ('17', '1', '我发布一个项目', '旅游户外', '哈哈哈哈', '12', '20', '2018-04-21 11:59:19');
INSERT INTO `project` VALUES ('15', '1', '1321', '企业服务', '321', '12', '10', '2018-04-19 10:53:44');

