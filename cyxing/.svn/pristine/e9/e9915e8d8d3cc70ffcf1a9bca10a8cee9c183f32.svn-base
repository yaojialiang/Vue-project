-- ----------------------------
-- Table structure for busistar
-- ----------------------------
DROP TABLE IF EXISTS `busistar`;
CREATE TABLE `busistar` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `headImg` varchar(255) NOT NULL COMMENT '头像',
  `starname` varchar(255) NOT NULL COMMENT '创业之星名字',
  `sex` varchar(255) DEFAULT NULL COMMENT '性别',
  `age` int(11) DEFAULT NULL COMMENT '年龄',
  `school` varchar(255) NOT NULL COMMENT '所属学校',
  `manifesto` varchar(255) DEFAULT NULL COMMENT '创业宣言',
  `mainproject` int(255) NOT NULL COMMENT '主要创业项目id',
  `otherproject` varchar(255) DEFAULT NULL COMMENT '其他创业项目',
  `story` longtext COMMENT '创业故事/经历',
  `type` varchar(255) NOT NULL DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Records of busistar
-- ----------------------------
INSERT INTO `busistar` VALUES ('1', 'https://gss1.bdstatic.com/9vo3dSag_xI4khGkpoWK1HF6hhy/baike/w%3D268%3Bg%3D0/sign=aa346c65ae64034f0fcdc50097f81e0c/8c1001e93901213f59c1ec6451e736d12e2e959c.jpg', '王小旭', '男', '22', '广州中医药大学', '不管做什么，希望自己都能做那个NumberOne，要不然就没有乐趣了。', '1', '广州九尾信息科技公司', '<p>在创业之前，王锐旭为了赚钱做过很多兼职：做保安、摆地摊，甚至做模特经纪人。在找兼职的过程中，经历了数次被骗，如“交培训费”、“交兼职服装费”、“交钱办打工卡”等等，这些惨痛的经历让王锐旭看到了大学生兼职的商机。再加上社团经历让王锐旭意识到校园外的大企业都迫切地想打开高校市场却找不到有效渠道。瞅准商机，他开始筹建他的校园推广团队，并慢慢催生了做“兼职猫”的念头。</p>\r\n<p>2012年4月一次与中移动经理的面谈是王锐旭创业的另一个转折。当时中移动要求团队工作人员超过50人，但他的团队才6人。不过，王锐旭拍着胸脯说人数绝对够。</p>\r\n<p>王锐旭开始疯狂地“招兵买马”，到当年9月，团队已经发展到80多人，人员几乎覆盖了大学城十所高校。如今回想，王锐旭也忍不住感叹“那时是冒了很大险，一旦考察不通过或者活动失败很可能面临违约”。最终他和团队一共营收10多万元，这也是王锐旭创业第一桶金，也赢得了人气和信誉。</p>\r\n<p>如今，“兼职猫”已经覆盖全国40多个城市，拥有100多万学生用户和3万多企业用户。王锐旭也成为了90后大学生创业的成功案例。但王锐旭并不愿止步于此。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('2', 'https://timg02.bdimg.com/timg?wapbaike&quality=60&size=b500_330crop=&cut_x=0&cut_y=3&cut_w=500&cut_h=330&sec=1349839550&di=d2174c7c2ce90699e59c6bf5637467e6&src=http://imgsrc.baidu.com/baike/pic/item/b151f8198618367af6f392fb27738bd4b31ce59a.jpg', '余小文', '男', '23', '广州大学华软软件学院', '对我来说，时间很值钱，我不想把我的大学浪费在无意义的事情上。况且，不爱上课、不常看书不代表我不爱学习。', '2', '广州周末网络科技有限公司；创立超级课程表-表表', '<p>2009年，余佳文进入广州大学华软软件学院开始了自己的大学生涯。2011年，余佳文的大学生活在继续，一星期有三十节课，基本记不住课程表，经常忘记在哪里上课。课上发现漂亮女生，也不敢主动要联系方式。这些日常小烦恼，给了余佳文创业灵感。他拉上几个朋友，组建了8个人的创业团队，成员都是清一色的大学生。2011年下半年，开始研发软件“超级课程表”。而由于课程软件“超级课程表”设计粗糙、功能单一、“小家子气”，其开发团队的“余佳文在广州大学华软软件学院遭到冷嘲热讽。余佳文信心满满地推出超级课程表时，已不再被老师看好，老师觉得超级课程表没什么技术含量，只有小孩子才会玩，余佳文接受不了别人的贬低。</p>\r\n<p>超级课程表的idea来源于包括余佳文在内的一群思维活跃的年轻人。“xtu one”是早期超级课程表的开发团队，而在超级课程表诞生之前，这个团队还开发出一款邮件客户端，“当时这个产品被学校的老师看好，不过在经营一段时间后，发现它并不能达到预期的效果，于是转卖。</p>\r\n<p>2012年，他成立了自己的公司——广州超级周末科技有限公司，同年8月，余佳文拿到了第一笔天使投资，同年10月将产品向全广州推广，13年1月份又获得了第二笔天使投资，有传言称，360董事长周鸿祎是超级课程表的早期天使投资人。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('3', 'https://gss3.bdstatic.com/7Po3dSag_xI4khGkpoWK1HF6hhy/baike/s%3D220/sign=6e64f9e0db1373f0f13f689d940e4b8b/1e30e924b899a901f32f3aad17950a7b0208f517.jpg', '灰星', '男', '20', '广东海洋大学', '创出一片天地', '3', '彩色星空信息科技公司；灰星创业空间', '<p>灰星毕业与广东海洋大学，大学期间对什么事都很感兴趣，担任班上班委，校团委干事，实践团队队长多个身份，做事认真负责。</p>\r\n<p>大学期间每年参加学校举办的创新创业大赛并获奖无数。</p>\r\n<p>大三时，自主策划了“创业星”大学生创业辅助移动平台，帮助众多大学学子完成了创业梦想！</p>\r\n<p>大四时，创办了“彩色星空信息科技公司”，成为一名90后CEO,并且设立了灰星创业空间，云集了数百位创业青年，开发了多项创业项目。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('4', 'https://gss2.bdstatic.com/9fo3dSag_xI4khGkpoWK1HF6hhy/baike/w%3D268%3Bg%3D0/sign=2e7cecf5d188d43ff0a996f44525b526/359b033b5bb5c9eac7b14863d239b6003bf3b3f4.jpg', '张小天', '男', '23', '北京大学', '改变大学生的就业观，从我做起', '4', '伏牛堂创始人；《六道轮回--历史上的亡国明君》', '<p>在北大法学院就读期间，张天一被评选为北大演讲十佳，并代表北大参加北京市大学生演讲比赛获得冠军。同时，他也担任北京大学法学院研究生会秘书长。</p>\r\n<p>张天一硕士期间的研究方向是国际金融法，有数篇学术论文发表在《金融法苑》等学术期刊上。</p>\r\n<p>张天一的另外一个业余爱好是写作，他自称自己有文人情节，历任《潮声》杂志主笔、中央人民广播电台《文化地产》栏目特约评论员，著有《六道轮回——历史上的亡国名君》、《90‘s不为乌合不从众》、《伏牛传》，并有数百篇杂文发表于《青年文摘》、《中国青年报》、《读者》等各大杂志、报纸。</p>\r\n<p>张天一于2014年7月份从北大毕业后，便创立了伏牛堂。伏牛堂主营湖南常德牛肉米粉，公司获得了一批顶级投资机构如险峰华为、真格基金、IDG等的投资。同时，公司也进驻了北京大学创业训练营，并作为北京大学创业学生代表入选“中关村创业新生代”。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('5', 'https://gss3.bdstatic.com/-Po3dSag_xI4khGkpoWK1HF6hhy/baike/w%3D268%3Bg%3D0/sign=4070b55806b30f24359aeb05f0aeb67e/a044ad345982b2b7eb5b27fd38adcbef77099bc6.jpg', '张小宁', '女', '22', '中国传媒大学', 'High,I am Super Girl!', '5', '前《时尚Cosmo digital》主编;云南省高考语文状元;注册了互联网公司“泡否”', '<p>2012年被《创业家杂志》评为8位90后先锋创业者之一 [3]  ；</p>\r\n<p>2013年12月14日，被中欧国际工商学院邀请到上海创业营做演讲 [8]  ；</p>\r\n<p>2014年3月，成为凤凰网新闻客户端两会期间代言人 [9]  ；</p>\r\n<p>2014年4月，2014品牌中国获“2014品牌女性新锐奖” [10]  ；</p>\r\n<p>2014年，被评为“中国地产10大风云人物” [11]  ；</p>\r\n<p>2015年，被评为“2015年值得关注的女性创业者” [12]  ；</p>\r\n<p>2015年6月，参与录制《鲁豫有约》 [13]  。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('6', 'https://gss0.bdstatic.com/-4o3dSag_xI4khGkpoWK1HF6hhy/baike/s%3D220/sign=6c1244f7fbf2b211e02e824cfa816511/ae51f3deb48f8c54e8c4e7ab3a292df5e0fe7f1a.jpg', '陈小欧', '男', '24', '南洋理工大学', '光辉岁月-我为自己代言', '6', '纽交所220余年历史上最年轻的上市公司CEO ；亚洲十大年轻富豪第六名', '<p>陈欧发现中国的广大女性消费者对于线上购买化妆品的信心不足，线上化妆品行业没有领头羊企业存在。化妆品就是新大陆。</p>\r\n<p>他总结出了三个“可行条件”。首先，电子商务在中国正在高速发展是不争的事实；其次，化妆品需求很大，但市场上还没有一个可信的化妆品网站；最后，做这个别的男人不好意思做的行业反倒给了自己机会。</p>\r\n<p>合伙人之间有了激烈的争吵，陈欧要做电商，戴雨森提议做社区。他们这边争执不休之际，国内刮起了团购热。陈欧提议先借着团购的方式做着玩，凭感觉一步一步来。由于公司的流动资金只剩下30万，只好一面继续着游戏广告业务，一面用了两天时间，在技术上让团美网（聚美优品前身）上了线。</p>\r\n<p>2010年3月31日，团美网作为中国首家专业女性团购网站上线，以正品平价形象口碑相传，在短时间内取得飞速发展。9月9日，团美网正式启用顶级域名，更名为聚美优品，成为国内领先的女性时尚限时折扣购物平台。</p>\r\n<p>陈欧将代理商的化妆品买断，存放在仓库，以限时团购的形式卖出，价格比专卖店低了4成。同年5月，陈欧全面停掉了之前的游戏内置广告业务，同时再次获得了来自徐小平的200万元投资。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('7', 'http://t12.baidu.com/it/u=454373747,77403088&fm=173&s=E89111D77ED873D051AC3CF503008030&w=400&h=255&img.JPEG', '陈小婷', '女', '24', '绍兴文理学院', '90后想做就要做，不要怕困难。', '7', '绍兴拉芙文化有限公司；浙江麦趣贸易有限公司', '<p>陈玉婷在校期间就荣获了2015年度绍兴市创业大赛的一等奖，绍兴市杰出青年，校第十一届卓越奖学金二等奖等多项荣誉，她的创业事迹曾被腾讯网、新浪网、浙江日报、绍兴日报等多家媒体关注报道。谈到创业的初衷时，陈玉婷说：“我不喜欢那种朝九晚五的工作，喜欢有足够的时间去陪伴家人，正是因为这点，我决定了要自己创业。”</p>\r\n<p>在校期间，陈玉婷辗转公益事业，公益红娘、关爱艾滋病等公益活动并在社会各界引起了反响，使她渐渐地成为了公众眼中的“网红”，在社会上有了一定的知名度。</p>\r\n<p>2014年12月，陈玉婷利用自己赚来的钱和父母提供的少量资金作为自己的创业资金，成立了绍兴拉芙文化传播有限公司，公司主营广告设计、制作，市场营销策划以及品牌服务等等。2016年1月，陈玉婷入驻学院创业园，同年12月又成立了浙江麦趣贸易有限公司，并凭借公司富有创意的项目在2015年第六届绍兴市大学生创业大赛中获得了一等奖。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('8', 'http://t12.baidu.com/it/u=3321029882,2417313105&fm=173&s=31933ED9C41AFBCC1089C8360300C013&w=640&h=446&img.JPEG', '朱小琪', '男', '23', '韶关学院', '厚德载物，脚踏实地，放低身段', '8', '牧马人园艺公司', '<p>在刚开始创业时，父母赞助了朱怿琪一两万，但这对他来说还远远不够。他凭借自己大学期间担任外联部部长积累的口才与经验，通过找投资的方法，解决了注册资金短缺的问题。2012年，他成立了牧马人园艺公司，公司主营园林景观设计。2016年1月，他加入了大学生创业园，并凭借创业项目获得2015年第六届绍兴市大学生创业大赛一等奖。</p>\r\n<p>目前，在只有十多名在职人员（业务经理，项目经理，工人等）的情况下，他的公司一年收入高达4、5百万，稳定的月营业额有50万上下。</p>\r\n<p>当比较在学校与在社会的心态时，朱怿琪表示大学给了他一个提前认识社会的机会，也给了他开放的环境，自由的空间与专业的创业指导。</p>\r\n<p>对于今后的规划，他觉得要稳步发展现在的事业，脚踏实地的走下去。当问及经验和忠告时，朱怿琪说：“我只说三个字：得吃苦！创业之路，必定是艰难和辛苦的，会有人不解，会有人抱怨。但我们年轻人要做的就是少抱怨、多问、多看、多积累”。</p>', 'busistar');
INSERT INTO `busistar` VALUES ('9', 'http://img1.imgtn.bdimg.com/it/u=4125477826,3369508015&fm=27&gp=0.jpg', '陈小甫', '男', '24', '浙江财经学院', '创业就是要更好地服务大家！', '9', '财经快递创始人', '<p>开办学校快递，给该校的学生提供了巨大的方便！</p>', 'busistar');
INSERT INTO `busistar` VALUES ('10', 'https://ss2.bdstatic.com/70cFvnSh_Q1YnxGkpoWK1HF6hhy/it/u=3097869544,3025929194&fm=27&gp=0.jpg', '鲁小星', '女', '19', '广东海洋大学', '创业是我的人生目标', '10', '微校园场景设计师；校十大歌手冠军', '<p>鲁小星喜欢创业，大二时，就和小伙伴开了家宅零宅食商铺，卖起了零食</p>\r\n<p>大三时，设计了微校园场景。</p>\r\n<p>大四时，旗下产品“微校园”获得了巨额天使投资资金！</p>', 'busistar');

